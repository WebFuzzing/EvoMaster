type Query {
    # Return a flower idenfified by its id
    flowersById(id: Int!): Flower
}
#A flower of the API
type Flower {
    #The id of the flower
    id: Int!
    #The name of the flower
    name: String!
    #The type of the flower
    type: String!
    #The color of the flower
    color: String!
    #The price of the flower
    price: Int!
}