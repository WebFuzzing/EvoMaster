type Query {
    # Return all flowers
    flowers: [Flower!]!
}

#A flower of the API
type Flower{
    #The id of the flower
    id: Int!,
    #The name of the flower
    name: String!
    #The type of the flower
    type: FlowerType!
    #The color of the flower
    color: String!
    #The price of the flower
    price: Int!
}

enum FlowerType{
    ROSES
    TULIPS
    LILIES
    LIMONIUM

}